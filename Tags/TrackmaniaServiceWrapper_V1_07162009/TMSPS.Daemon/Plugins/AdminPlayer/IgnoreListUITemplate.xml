<?xml version="1.0" encoding="utf-8" ?>
<Settings>
	<FirstEntryTopMargin>-4</FirstEntryTopMargin>
	<EntryHeight>2.75</EntryHeight>
	<MaxEntriesPerPage>18</MaxEntriesPerPage>

	<Templates>
		<EntryTemplate>
			<frame posn="0 {[Y]} 0">
				<format textsize="2.5"/>

				<label posn="1 0 0.1" sizen="6 2.5" text="{[Position]}." />
				<label posn="8 0 0.1" sizen="18 2.5" text="{[Nickname]}" />
				<label posn="27 0 0.1" sizen="18 2.5" text="{[Login]}" />
				<quad posn="46 0 0.1" sizen="2.5 2.5" style="Icons64x64_1" substyle="Close" action="{[RemoveActionID]}"/>
			</frame>
		</EntryTemplate>

		<SinglePageTemplate>
			<manialink id="{[ManiaLinkID]}">
				<frame posn="-25 34 0">

					<quad posn="0 0 0" sizen="50 65" style="Bgs1" substyle="BgWindow1"/>

					<frame posn="0.25 -0.25 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="IconPlayers"/>
						<label posn="4 -1 0.1" sizen="20 3" style="TextRaceChat" text="Manage Ignorelist"/>
						<quad posn="46.35 -0.6 0.1" sizen="3 3" style="Icons64x64_1" substyle="QuitRace" action="{[CloseActionID]}"/>
					</frame>

					<frame posn="0.25 -4.8 0.1">
						<quad posn="0 0 0" sizen="49.4 55" style="Bgs1" substyle="BgTitle3"/>

						<label posn="1 -1 0.1" text="$oNr." textsize="2.5"/>
						<label posn="8 -1 0.1" text="$oNickname" textsize="2.5"/>
						<label posn="27 -1 0.1" text="$oLogin" textsize="2.5"/>

						<PlayerPlaceHolder />
					</frame>

					<frame posn="0.25 -60.5 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>
						<quad posn="5.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>

						<label posn="25 -1.8 0.1" sizen="30 3" style="TextRaceChat" text="Page: 1/1" halign="center" valign="center"/>

						<quad posn="41.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>
						<quad posn="46.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>
					</frame>
				</frame>
			</manialink>
		</SinglePageTemplate>

		<FirstPageTemplate>
			<manialink id="{[ManiaLinkID]}">
				<frame posn="-25 34 0">

					<quad posn="0 0 0" sizen="50 65" style="Bgs1" substyle="BgWindow1"/>

					<frame posn="0.25 -0.25 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="IconPlayers"/>
						<label posn="4 -1 0.1" sizen="20 3" style="TextRaceChat" text="Manage Ignorelist"/>
						<quad posn="46.35 -0.6 0.1" sizen="3 3" style="Icons64x64_1" substyle="QuitRace" action="{[CloseActionID]}"/>
					</frame>

					<frame posn="0.25 -4.8 0.1">
						<quad posn="0 0 0" sizen="49.4 55" style="Bgs1" substyle="BgTitle3"/>

						<label posn="1 -1 0.1" text="$oNr." textsize="2.5"/>
						<label posn="8 -1 0.1" text="$oNickname" textsize="2.5"/>
						<label posn="27 -1 0.1" text="$oLogin" textsize="2.5"/>

						<PlayerPlaceHolder />
					</frame>

					<frame posn="0.25 -60.5 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>
						<quad posn="5.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold"/>

						<label posn="25 -1.8 0.1" sizen="30 3" style="TextRaceChat" text="Page: 1/{[MaxPage]}" halign="center" valign="center"/>

						<quad posn="41.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowNext" action="{[NextPageActionID]}"/>
						<quad posn="46.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowLast" action="{[LastPageActionID]}"/>
					</frame>
				</frame>
			</manialink>
		</FirstPageTemplate>

		<MiddlePageTemplate>
			<manialink id="{[ManiaLinkID]}">
				<frame posn="-25 34 0">

					<quad posn="0 0 0" sizen="50 65" style="Bgs1" substyle="BgWindow1"/>

					<frame posn="0.25 -0.25 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="IconPlayers"/>
						<label posn="4 -1 0.1" sizen="20 3" style="TextRaceChat" text="Manage Ignorelist"/>
						<quad posn="46.35 -0.6 0.1" sizen="3 3" style="Icons64x64_1" substyle="QuitRace" action="{[CloseActionID]}"/>
					</frame>

					<frame posn="0.25 -4.8 0.1">
						<quad posn="0 0 0" sizen="49.4 55" style="Bgs1" substyle="BgTitle3"/>

						<label posn="1 -1 0.1" text="$oNr." textsize="2.5"/>
						<label posn="8 -1 0.1" text="$oNickname" textsize="2.5"/>
						<label posn="27 -1 0.1" text="$oLogin" textsize="2.5"/>

						<PlayerPlaceHolder />
					</frame>

					<frame posn="0.25 -60.5 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowFirst" action="{[FirstPageActionID]}"/>
						<quad posn="5.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowPrev" action="{[PrevPageActionID]}"/>

						<label posn="25 -1.8 0.1" sizen="30 3" style="TextRaceChat" text="Page: {[CurrentPage]}/{[MaxPage]}" halign="center" valign="center"/>

						<quad posn="41.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowNext" action="{[NextPageActionID]}"/>
						<quad posn="46.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowLast" action="{[LastPageActionID]}"/>
					</frame>
				</frame>
			</manialink>
		</MiddlePageTemplate>

		<LastPageTemplate>
			<manialink id="{[ManiaLinkID]}">
				<frame posn="-25 34 0">

					<quad posn="0 0 0" sizen="50 65" style="Bgs1" substyle="BgWindow1"/>

					<frame posn="0.25 -0.25 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="IconPlayers"/>
						<label posn="4 -1 0.1" sizen="20 3" style="TextRaceChat" text="Manage Ignorelist"/>
						<quad posn="46.35 -0.6 0.1" sizen="3 3" style="Icons64x64_1" substyle="QuitRace" action="{[CloseActionID]}"/>
					</frame>

					<frame posn="0.25 -4.8 0.1">
						<quad posn="0 0 0" sizen="49.4 55" style="Bgs1" substyle="BgTitle3"/>

						<label posn="1 -1 0.1" text="$oNr." textsize="2.5"/>
						<label posn="8 -1 0.1" text="$oNickname" textsize="2.5"/>
						<label posn="27 -1 0.1" text="$oLogin" textsize="2.5"/>

						<PlayerPlaceHolder />
					</frame>

					<frame posn="0.25 -60.5 0.1">
						<quad posn="0 0 0" sizen="49.4 4" style="Bgs1" substyle="BgTitle3_2"/>
						<quad posn="0.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowFirst" action="{[FirstPageActionID]}"/>
						<quad posn="5.4 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="ArrowPrev" action="{[PrevPageActionID]}"/>

						<label posn="25 -1.8 0.1" sizen="30 3" style="TextRaceChat" text="Page: {[CurrentPage]}/{[MaxPage]}" halign="center" valign="center"/>

						<quad posn="41.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold" />
						<quad posn="46.1 -0.65 0.1" sizen="3 3" style="Icons64x64_1" substyle="StarGold" />
					</frame>
				</frame>
			</manialink>
		</LastPageTemplate>
	</Templates>
</Settings>